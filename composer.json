{
    "name": "kematjaya/serial-number-bundle",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Nur Hidayatullah",
            "email": "kematjaya0@gmail.com"
        }
    ],
    "repositories":[
        {
            "type": "vcs",
            "url": "https://github.com/kematjaya0/serial-number.git"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/dependency-injection": "^4.0|^5.0|^5.1",
        "symfony/config": "^4.0|^5.0|^5.1",
        "doctrine/common": "*",
        "kematjaya/serial-number": "^1.0",
        "symfony/http-kernel": "^4.0|^5.0|^5.1",
        "symfony/framework-bundle": "^4.0|^5.0|^5.1",
        "symfony/security-bundle": "^4.0|^5.0|^5.1",
        "symfony/console": "^4.0|^5.0|^5.1",
        "symfony/http-foundation": "^4.0|^5.0|^5.1",
        "symfony/routing": "^4.0|^5.0|^5.1",
        "symfony/security-core": "^4.0|^5.0|^5.1"
    },
    "autoload": {
        "psr-4": {
            "Kematjaya\\SerialNumberBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kematjaya\\SerialNumberBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "symfony/browser-kit": "^4.0|^5.0",
        "doctrine/doctrine-fixtures-bundle": "^3.3"
    }
}
